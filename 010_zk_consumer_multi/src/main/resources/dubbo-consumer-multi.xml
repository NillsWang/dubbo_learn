<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo
       http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <!--声明dubbo服务消费者者的名称：保证唯一性-->
    <dubbo:application name="010_zk_consumer_multi"/>

    <!--指定注册中心-->
    <dubbo:registry address="zookeeper://localhost:2181"/>

    <!--引用远程接口服务-->
    <dubbo:reference id="userService1"
            interface="com.nilswang.dubbo.service.UserService"
            url="dubbo://localhost:20880" version="1.0.0"/>

    <dubbo:reference id="userService2"
            interface="com.nilswang.dubbo.service.UserService"
            url="dubbo://localhost:20880" version="2.0.0"/>

</beans>